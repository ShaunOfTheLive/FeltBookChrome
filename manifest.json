{
  "manifest_version": 2,

  "name": "FeltBook",
  "description": "This extension materializes Facebook",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://www.facebook.com"
  ]
}
